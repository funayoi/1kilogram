{
    "name": "1kilogram",
    "description": "Instagram viewer for chrome",
    "version": "1",
    "app": {
        "launch": {
          "local_path": "main.html"
        }
    },
    "permissions": [
        "tabs",
        "https://api.instagram.com/",
        "https://instagram.com/",
        "http://instagram.com/"
    ]/*,
    "icons": {
      "16": "icon_16.png",
      "128": "icon_128.png"
    }
    */
}

